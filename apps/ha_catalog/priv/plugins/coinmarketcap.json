{
    "name": "Coinmarketcap",
    "description": "Market and social data for listed crypto currencies",
    "query": {
        "from": {
            "alias": "response",
            "source": {
                "type": "fetch",
                "config": {
                    "url": "https://coinmarketcap.com"
                }
            }
        },
        "select": {
            "row": [
                {
                    "type": "ref",
                    "value": "response.body"
                },
                {
                    "type": "html_parse_list",
                    "selector": ".currency-name-container"
                },
                {
                    "type": "unnest"
                }
            ]
        }
    }
}
